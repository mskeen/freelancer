<%- if @api_key.errors.any? %>
  $('#modal-window').html('<%= escape_javascript(render('modal')) %>');
<%- else %>
  $('#modal-window').modal('hide')
  $("#api-key-<%= @api_key.id %>").replaceWith('<%= escape_javascript(render partial: "api_keys/row", locals: { api_key: @api_key }) %>');
<%- end %>
