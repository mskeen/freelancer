<% title 'Event Tracking' %>
<% heading 'Event Tracking' %>

<h1>Listing event_trackers</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Organization</th>
      <th>Name</th>
      <th>Email</th>
      <th>Notes</th>
      <th>Interval cd</th>
      <th>Token</th>
      <th>Sort order</th>
      <th>Is paused</th>
      <th>Is deleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_trackers.each do |event_tracker| %>
      <tr>
        <td><%= event_tracker.user %></td>
        <td><%= event_tracker.organization %></td>
        <td><%= event_tracker.name %></td>
        <td><%= event_tracker.email %></td>
        <td><%= event_tracker.notes %></td>
        <td><%= event_tracker.interval_cd %></td>
        <td><%= event_tracker.token %></td>
        <td><%= event_tracker.sort_order %></td>
        <td><%= event_tracker.is_paused %></td>
        <td><%= event_tracker.is_deleted %></td>
        <td><%= link_to 'Show', event_tracker %></td>
        <td><%= link_to 'Edit', edit_event_tracker_path(event_tracker) %></td>
        <td><%= link_to 'Destroy', event_tracker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Tracker', new_event_tracker_path %>
